<Page x:Class="RentOfCar.Pages.Perehod"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RentOfCar.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Perehod">

    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Margin="10" x:Name="ProductBtn" Click="CarBtn_Click">Список автомобилей</Button>
            <Button Margin="10" x:Name="OrderBtn" Click="OrderBtn_Click">Список заказов</Button>
        </StackPanel>
    </Grid>
</Page>
